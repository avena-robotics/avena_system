<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>compose_items</name>
  <version>0.1.0</version>
  <description>Contains logic composing items data, such as grouping items with their elements, and finding items correspondence in multiple cameras.</description>
  <maintainer email="karol.j.frankowski@gmail.com">avena</maintainer>
  <license>Apache License 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <!-- <depend>action_tutorials_interfaces</depend> -->
  <depend>rclcpp</depend>
  <depend>rclcpp_action</depend>
  <depend>rclcpp_components</depend>
  <depend>sensor_msgs</depend>
  <depend>custom_interfaces</depend>
  <depend>helpers_vision</depend>
  <depend>helpers_commons</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
